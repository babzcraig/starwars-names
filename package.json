{
  "name": "babs-starwars-names",
  "version": "0.0.0-development",
  "description": "Get random starwars names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test:watch": "mocha src/index.test.js -w",
    "test": "istanbul cover -x 'src/*.test.js' _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babzcraig/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Babs Craig <babscraig@gmail.com> (https://babscraig.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/babzcraig/starwars-names/issues"
  },
  "homepage": "https://github.com/babzcraig/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "semantic-release": "15.13.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
